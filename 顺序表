#pragma once
#include<stdio.h>
#include<stdlib.h>
#include<string.h>

//顺序表头插
typedef struct SeqList {
	int *array;
	int size;
	int capacity;
}   SeqList;

//扩容的头插
void PushFront(SeqList *s, int v) {
	//住不下了
	if (s->size >= s->capacity) {
		////找新房（需要扩容）
		int capacity = s->capacity * 2;
		int *array = (int *)malloc(sizeof(int)* capacity)
			//搬家
		for (int i = 0; i < s - size; i++) {
			array[i] = s->array[i];
		}
		//退租老房子
		free(s->array);
		//去居委会变更租房信息
		s->array = array;
		s->capacity = capacity;
	}
	//把所有的数据往后搬一格 s->size    头插过程
	for (int i = s->size; i>0; i--) {
		s - array[i] = s->array[i - 1];
	}
	s->array[0] = v;
	s->size++;
}


